{
  "name": "farmdashboard-front",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "local": "npm run config:dev && ng serve --port 4200",
    "build": "ng build",
    "config:dev": "ts-node ./scripts/setenv.ts --configuration=dev",
    "config:prod": "ts-node ./scripts/setenv.ts --configuration=prod",
    "build:dev": "npm run config:dev && ng build --configuration development",
    "build:prod": "npm run config:prod && ng build --configuration production",
    "test": "ng test",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "e2e": "ng e2e",
    "bsc": "ng build --configuration production && DEPLOYMENT_ENV=bsc heroku local -p 5000",
    "stage": "ng build --configuration production && DEPLOYMENT_ENV=stage heroku local -p 5000",
    "prod": "ng build --configuration production && DEPLOYMENT_ENV=prod heroku local -p 5000",
    "heroku-postbuild": "npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.1",
